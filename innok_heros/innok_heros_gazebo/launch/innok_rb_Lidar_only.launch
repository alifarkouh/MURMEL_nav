<?xml version="1.0"?>
<launch>
  <!-- arg name="paused" default="false"/>
  <arg name="use_sim_time" default="true"/>
  <arg name="gui" default="true"/>
  <arg name="wheels" default="4"/>
  <arg name="tyre_type" default=""/-->	

  <param name="robot_description"
	 command="$(find xacro)/xacro --inorder '$(find innok_heros_description)/urdf/innok_heros_4w.xacro'" />

  <!-- send fake joint values -->
  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher"/>

  <!-- Combine joint values -->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>

  <!-- Show in Rviz   -->
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find innok_heros_gazebo)/rviz/rviz_Lidar_only.rviz"/>
  
</launch>
